@import '../common/index.wxss';

.van-swipe-cell {
    position: relative;
    overflow: hidden
}

.van-swipe-cell__left{
        position: absolute;
        top: 2px;
        height: 120%
}
.van-swipe-cell__right {
    position: absolute;
    top: 2px;
    height: 120%
}

.van-swipe-cell__left {
    left: 0;
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0)
}

.van-swipe-cell__right {
    right: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0)
}