<!--pages/finished/finished.wxml-->
<wxs module="result" src="resultProcess.wxs"></wxs>
<view class="page">
    <view class="page__hd">
        <view class="page__title">Submission</view>
        <view class="page__desc">提交记录</view>
    </view>
    <view class="page__bd">
        <view class="weui-grids">
            <block wx:for="{{grids}}">
                <navigator url="" class="weui-grid" hover-class="weui-grid_active">
                    <text class="" selectable="false" space="false" decode="false">
                        <text class="" selectable="false" space="false" decode="false">
                            题目编号： {{item.problemId}}
                        </text>
                        <!-- <text class="" selectable="false" space="false" decode="false">
                            题目结果：{{item.result == 0}}
                        </text> -->
                        <text class="" selectable="false" space="false" decode="false">
                            提交结果：{{result.numberToResult(item.result)}}
                        </text>
                    </text>
                    <view class="icon-box">
                        <icon type="{{result.numberToIcon(item.result)}}" size="32"></icon>
                        <view class="icon-box__ctn">
                        </view>
                    </view>
                    <!-- <view class="weui-grid__label">Grid</view> -->
                </navigator>
            </block>
        </view>
    </view>
</view>
